def permutations(value,coin):
  ways = 0
  arr = [] #looked up array function https://www.w3schools.com/python/python_arrays.asp
  for i in range(value+1):
    arr.append(0)
  arr[0] = 1
  
  for coin in coins:
    for i in range(1, value+1):
      if(i-coin >= 0):
        if arr[i-coin]>0:
          arr[i]+=arr[i-coin]
return arr[value]
coin = [1,2,5,10,20,50,100,200]
print(permutations(200,coin))
